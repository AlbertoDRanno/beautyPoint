<!DOCTYPE html>
<html lang="en">
  <%- include('partials/head.ejs', {title: "Encontrá eso que Querés"})%>

  <body>
    <%- include('partials/header.ejs')%>
    <main class="home-main">
      <ul>
        <% for( let i = 0; i < categories.length; i++ ) { %>
        <li>
          <%= categories[i].description %>

          <ul>
            <% for( let j = 0; j < categories[i].productosC.length; j++ ) { %>

            <li><%= categories[i].productosC[j].name %></li>
            <% } %>
          </ul>
        </li>
        <% } %>
      </ul>
    </main>
  </body>
</html>
